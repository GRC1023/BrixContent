              "presenterConfig": {
                "numberFormat": "none"
              },
              "choices": [
                {
                  "answerKey": "answer_1",
                  "content": "The act enforced collection of English customs on tobacco."
                },
                {
                  "answerKey": "answer_2",
                  "content": "The act allowed planters from various European nations to enter America."
                },
                {
                  "answerKey": "answer_3",
                  "content": "The act enforced free trade and increased competition in the market."
                },
                {
                  "answerKey": "answer_4",
                  "content": "The act introduced restrictions on the amount of land that one could own."
                }
              ]
            },
            "answerKey": {
              "assessmentType": "multiplechoice",
              "answers": {
                "answer_3": {
                  "score": 0.0,
