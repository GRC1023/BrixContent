              "presenterConfig": {
                "numberFormat": "none"
              },
              "choices": [
                {
                  "answerKey": "answer_1",
                  "content": "It chose Marcus A. Hanna as the presidential candidate."
                },
                {
                  "answerKey": "answer_2",
                  "content": "It agreed to form an alliance with the People’s party."
                },
                {
                  "answerKey": "answer_3",
                  "content": "It favored the gold standard against the free coinage of silver."
                },
                {
                  "answerKey": "answer_4",
                  "content": "It decided to abolish the tariffs that existed in the United States."
                }
              ]
            },
