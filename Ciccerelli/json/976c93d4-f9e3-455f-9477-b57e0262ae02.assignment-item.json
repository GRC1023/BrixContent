            "config": {
              "id": "QUE57465",
              "question": "Which of the following characteristics best describes differences between bulimia nervosa and anorexia nervosa?\n\u003cbr /\u003e",
              "order": "randomized",
              "presenterType": "RadioGroup",
              "presenterConfig": {
                "numberFormat": "none"
              },
              "choices": [
                {
                  "answerKey": "answer_1",
                  "content": "Individuals with anorexia do not have as severe health risks that individual with bulimia have."
                },
                {
                  "answerKey": "answer_2",
                  "content": "Individuals with bulimia may have a normal body weight whereas those with anorexia tend to be severely under their expected body weight."
                },
                {
                  "answerKey": "answer_3",
                  "content": "Individuals with anorexia have been known to binge like those with bulimia on occasion."
                },
                {
                  "answerKey": "answer_4",
                  "content": "Anorexia tends to occur in early adulthood while bulimia often starts in early adolescence."
                }
              ]
            },
