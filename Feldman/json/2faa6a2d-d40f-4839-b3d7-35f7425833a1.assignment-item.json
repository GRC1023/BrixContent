            "config": {
              "id": "QUE62688",
              "question": "Which of the following hypotheses may explain the higher incidence of depression in adolescent girls?\n\u003cbr /\u003e",
              "order": "randomized",
              "presenterType": "RadioGroup",
              "presenterConfig": {
                "numberFormat": "none"
              },
              "choices": [
                {
                  "answerKey": "answer_1",
                  "content": "Stress is more pronounced for girls due to the conflicting demands of the traditional female role."
                },
                {
                  "answerKey": "answer_2",
                  "content": "Females traditionally are less able to deal with stress than are males."
                },
                {
                  "answerKey": "answer_3",
                  "content": "Females are more likely to turn to drugs and alcohol than are males; therefore, their problems are more apparent."
                },
                {
                  "answerKey": "answer_4",
                  "content": "Parents are more concerned and protective of their daughters than their sons; therefore, they notice depression in girls earlier."
                }
              ]
            },
