            "config": {
              "id": "QUE61663",
              "question": "In 1996, Bill Clinton’s chances of being reelected to the presidency would have been slim without __________.\n\u003cbr /\u003e",
              "order": "randomized",
              "presenterType": "RadioGroup",
              "presenterConfig": {
                "numberFormat": "none"
              },
              "choices": [
                {
                  "answerKey": "answer_1",
                  "content": "his promises to diminish the scope of government"
                },
                {
                  "answerKey": "answer_2",
                  "content": "investigations into his personal and professional life"
                },
                {
                  "answerKey": "answer_3",
                  "content": "the Republican promise to increase social services"
                },
                {
                  "answerKey": "answer_4",
                  "content": "the refusal of Republicans in Congress to pass his budget"
                }
              ]
            },
            "answerKey": {
              "assessmentType": "multiplechoice",
              "answers": {
