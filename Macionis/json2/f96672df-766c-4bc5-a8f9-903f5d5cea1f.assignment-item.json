            "config": {
              "id": "QUE57772",
              "question": "Which of the following women is at the greatest risk of violence?\n\u003cbr /\u003e",
              "order": "randomized",
              "presenterType": "RadioGroup",
              "presenterConfig": {
                "numberFormat": "none"
              },
              "choices": [
                {
                  "answerKey": "answer_1",
                  "content": "Violet, who lives with her husband and three young children. Both she and her husband work to support their family, but together they earn only $30,000 a year."
                },
                {
                  "answerKey": "answer_2",
                  "content": "Selene, who lives with her parents, who help raise her infant daughter. During the day, Selene attends high school, and spends time with her family at night.  "
                },
                {
                  "answerKey": "answer_3",
                  "content": "Cindy, who has been married for 35 years. Over time, she and her husband have grown a successful business."
                },
                {
                  "answerKey": "answer_4",
                  "content": "Bonnie, who is divorced. She lives with her teenage son and works as a defense attorney, sending her ex-husband a large alimony each month."
                }
              ]
            },
            "answerKey": {
              "assessmentType": "multiplechoice",
              "answers": {
