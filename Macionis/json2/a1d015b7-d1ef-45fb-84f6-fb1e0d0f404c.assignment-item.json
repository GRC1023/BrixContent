            "config": {
              "id": "QUE59619",
              "question": "How is farming today different from what it was like in 1800?\n\u003cbr /\u003e",
              "order": "randomized",
              "presenterType": "RadioGroup",
              "presenterConfig": {
                "numberFormat": "none"
              },
              "choices": [
                {
                  "answerKey": "answer_1",
                  "content": "Today, there is still an equal amount of manual labor and skilled labor in farming."
                },
                {
                  "answerKey": "answer_2",
                  "content": "In 1800, most U.S. workers were farmers; today, only about 1 percent of workers are farmers."
                },
                {
                  "answerKey": "answer_3",
                  "content": "Farming is following an evolutionary trend that is the opposite of that of other occupations."
                },
                {
                  "answerKey": "answer_4",
                  "content": "Farmers are still strong in number; they just produce more crops than they did in 1800."
                }
