            "config": {
              "id": "QUE62895",
              "question": "Which statement reflects the changes in women’s social positions since 1970?\n\u003cbr /\u003e",
              "order": "randomized",
              "presenterType": "RadioGroup",
              "presenterConfig": {
                "numberFormat": "none"
              },
              "choices": [
                {
                  "answerKey": "answer_1",
                  "content": "Women are now equally represented in politics, which seemed impossible in 1970."
                },
                {
                  "answerKey": "answer_2",
                  "content": "Women are equally paid for the work they do that men also do."
                },
                {
                  "answerKey": "answer_3",
                  "content": "Though women are not yet equally represented, the percentages of women have increased in colleges and professions."
                },
                {
                  "answerKey": "answer_4",
                  "content": "Though women have far exceeded men in professional life, they still make the same percentage of a man’s salary from 1970."
                }
              ]
            },
