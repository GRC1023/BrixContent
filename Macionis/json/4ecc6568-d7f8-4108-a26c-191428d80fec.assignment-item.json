              "presenterType": "RadioGroup",
              "presenterConfig": {
                "numberFormat": "none"
              },
              "choices": [
                {
                  "answerKey": "answer_1",
                  "content": "The influx put a strain on the country and resulted in a depression."
                },
                {
                  "answerKey": "answer_2",
                  "content": "The average wage increased with the entry of skilled labor."
                },
                {
                  "answerKey": "answer_3",
                  "content": "The Protestant values and ethics of America were replaced by Catholic culture."
                },
                {
                  "answerKey": "answer_4",
                  "content": "Cheap labor facilitated industrialization in America."
                }
              ]
            },
            "answerKey": {
              "assessmentType": "multiplechoice",
              "answers": {
