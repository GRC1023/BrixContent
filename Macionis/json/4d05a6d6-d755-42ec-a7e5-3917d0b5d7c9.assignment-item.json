              "presenterType": "RadioGroup",
              "presenterConfig": {
                "numberFormat": "none"
              },
              "choices": [
                {
                  "answerKey": "answer_1",
                  "content": "The Native American peoples had established colonies as they came across the land bridge from Asia."
                },
                {
                  "answerKey": "answer_2",
                  "content": "Africans also established colonies in the Americas."
                },
                {
                  "answerKey": "answer_3",
                  "content": "Most of the immigrant population consisted of slaves from Africa."
                },
                {
                  "answerKey": "answer_4",
                  "content": "The New World was sparsely populated with native peoples."
                }
              ]
            },
