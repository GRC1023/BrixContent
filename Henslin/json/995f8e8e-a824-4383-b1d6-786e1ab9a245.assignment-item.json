            "config": {
              "id": "QUE64948",
              "question": "Why is it difficult to determine the exact onset of adulthood?\n\u003cbr /\u003e",
              "order": "randomized",
              "presenterType": "RadioGroup",
              "presenterConfig": {
                "numberFormat": "none"
              },
              "choices": [
                {
                  "answerKey": "answer_1",
                  "content": "Biologists are not in agreement on the criteria for physical maturity in humans."
                },
                {
                  "answerKey": "answer_2",
                  "content": "Inequality causes differences in growth rates between genders."
                },
                {
                  "answerKey": "answer_3",
                  "content": "The social experiences that constitute childhood vary across cultures."
                },
                {
                  "answerKey": "answer_4",
                  "content": "Individuals grow and mature physically and mentally at different rates."
                }
              ]
            },
            "answerKey": {
              "assessmentType": "multiplechoice",
              "answers": {
