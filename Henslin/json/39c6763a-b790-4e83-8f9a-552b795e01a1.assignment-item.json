              "presenterType": "RadioGroup",
              "presenterConfig": {
                "numberFormat": "none"
              },
              "choices": [
                {
                  "answerKey": "answer_1",
                  "content": "Personal space is an American concept."
                },
                {
                  "answerKey": "answer_2",
                  "content": "Personal space is biologically innate."
                },
                {
                  "answerKey": "answer_3",
                  "content": "Personal space can be seen as a sign of sexual interest or disinterest."
                },
                {
                  "answerKey": "answer_4",
                  "content": "Personal space is largely cultural."
                }
