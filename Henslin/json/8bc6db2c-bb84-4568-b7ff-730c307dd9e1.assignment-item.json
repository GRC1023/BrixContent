            "config": {
              "id": "QUE64909",
              "question": "For what reason do sociologists study twins who were separated at birth?\n\u003cbr /\u003e",
              "order": "randomized",
              "presenterType": "RadioGroup",
              "presenterConfig": {
                "numberFormat": "none"
              },
              "choices": [
                {
                  "answerKey": "answer_1",
                  "content": "They are biologically similar but have different upbringings."
                },
                {
                  "answerKey": "answer_2",
                  "content": "They are generally the product of broken homes."
                },
                {
                  "answerKey": "answer_3",
                  "content": "They are identical both physically and in terms of personality."
                },
                {
                  "answerKey": "answer_4",
                  "content": "They allow for comparisons of the social development of twins to that of single-birth children."
