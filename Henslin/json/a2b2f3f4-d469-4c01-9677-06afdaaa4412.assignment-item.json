            "config": {
              "id": "QUE65252",
              "question": "What significant change has taken place in U.S. families regarding child care?\n\u003cbr /\u003e",
              "order": "randomized",
              "presenterType": "RadioGroup",
              "presenterConfig": {
                "numberFormat": "none"
              },
              "choices": [
                {
                  "answerKey": "answer_1",
                  "content": "Husbands are taking more responsibility for child care than their wives."
                },
                {
                  "answerKey": "answer_2",
                  "content": "Both husbands and wives are taking less responsibility for child care, allowing nannies and organized child care facilities to handle most of the child care duties."
                },
                {
                  "answerKey": "answer_3",
                  "content": "Despite the increase in the number of women in the workforce, husbands still refer to child care as “women’s work” and do not participate."
                },
                {
                  "answerKey": "answer_4",
                  "content": "Both husbands and wives are spending more time handling child care."
                }
              ]
            },
