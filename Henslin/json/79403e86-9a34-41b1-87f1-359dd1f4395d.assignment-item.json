            "config": {
              "id": "QUE59044",
              "question": "Why is it important to share the results of your research even if your hypothesis is not proven?\n\u003cbr /\u003e",
              "order": "randomized",
              "presenterType": "RadioGroup",
              "presenterConfig": {
                "numberFormat": "none"
              },
              "choices": [
                {
                  "answerKey": "answer_1",
                  "content": "Researchers who do not report their results are often shunned by the scientific community."
                },
                {
                  "answerKey": "answer_2",
                  "content": "It is required that all research be published in a scientific journal or book."
                },
                {
                  "answerKey": "answer_3",
                  "content": "So that others will know not to engage in research on a topic that has already been proven or disproven."
                },
                {
                  "answerKey": "answer_4",
                  "content": "Other researchers can then repeat your research to see if they come up with similar results."
                }
              ]
            },
